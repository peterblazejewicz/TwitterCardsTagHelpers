{
  "name": "TwitterCardTagHelpers.Web",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.5",
    "highlightjs": "^8.9.1",
    "jquery": "^2.1.4"

  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/*.**",
        "dist/fonts/*.*",
        "dist/js/bootstrap{.js,.min.js}"
      ]
    },
    "highlightjs": {
      "main": [
        "highlight.pack{.min.js,.js}",
        "styles/default.css",
        "styles/zenburn.css"
      ]
    },
    "jquery": {
      "main": "dist/*.**"
    }
  }
}
